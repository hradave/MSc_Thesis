#!/bin/bash
# cp wsi-s to a folder

#DESTDIR=/nobackup/data/davhr856/data/TCGA_DX/dataset

for VARIABLE in LGG/97831ac4-9d22-48df-ab80-e5cab3ebe675/TCGA-HW-A5KJ-01Z-00-DX1.565C251B-6B1B-4589-99E6-DD1ED951CA0C.svs	LGG/5a6aa6a1-556c-4481-ac06-3f2feb853296/TCGA-DB-A64Q-01Z-00-DX1.CD3937C2-015A-490D-B6A6-BAEEFFDC0747.svs	GBM/3465da7c-9828-4c38-960a-ef0706d453c0/TCGA-08-0529-01Z-00-DX1.c0aefbea-1aa2-4091-b320-dbdab0964b6d.svs	GBM/d1b07168-e805-493d-855a-7c3d580eadf2/TCGA-06-1087-01Z-00-DX2.1f91f05a-f277-4c98-9955-37e0c83b745f.svs	GBM/0961b4a7-523d-457c-8a49-fb099fb49db9/TCGA-06-0213-01Z-00-DX4.2e849308-80d0-4cd1-92c2-8649c790095c.svs	GBM/34bd0d93-557a-4425-a6f0-ee9e0c8ded53/TCGA-06-0206-01Z-00-DX2.df39f5d7-9d4b-489d-8f86-1a9ae9793ab7.svs
do
  full_path="/nobackup/data/davhr856/data/TCGA_DX/original/$VARIABLE"
  folders=$( echo $VARIABLE | cut -c1-40 )
  new_folder="/nobackup/data/davhr856/data/TCGA_DX/dataset_all/$folders"
  #echo $folders
  mkdir -p $new_folder
  scp $full_path $new_folder
done

#scp /nobackup/data/davhr856/data/TCGA_DX/original/GBM/41b91dfe-8fa7-497d-9a68-1a4d6d9c23e5/TCGA-06-0157-01Z-00-DX2.8ff48e0c-0470-4055-af66-a1a40e587d50.svs $DESTDIR
#scp /nobackup/data/davhr856/data/TCGA_DX/original/GBM/ae156825-5394-4043-a3e9-69c71d875c6f/TCGA-06-0130-01Z-00-DX1.0391b65f-4e1d-4444-abb0-e5804606d461.svs $DESTDIR
